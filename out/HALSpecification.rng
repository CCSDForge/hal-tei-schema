                  <ref name="publicationStmt"/>
               </optional>
               <optional>
                  <ref name="seriesStmt"/>
               </optional>
               <optional>
                  <ref name="notesStmt"/>
               </optional>
               <zeroOrMore>
                  <ref name="sourceDesc"/>
               </zeroOrMore>
               <zeroOrMore>
                  <ref name="profileDesc"/>
               </zeroOrMore>
            </group>
            <group>
               <ref name="fileDesc"/>
               <ref name="profileDesc"/>
            </group>
         </choice>
      </element>
